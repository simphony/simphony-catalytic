FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      thetaDefault;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    wedge1
    {
        type            wedge;
    }
    wedge2
    {
        type            wedge;
    }
    wedge3
    {
        type            wedge;
    }
    wedge4
    {
        type            wedge;
    }
    inertWall
    {
        type            zeroGradient;
    }
    reactingWall
    {
        type            fixedValue;
        value           uniform 1;
    }
    tubeWall
    {
        type            zeroGradient;
    }
}


